{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "builder/const.go"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "libressl/portable",
      "matchStrings": [
        "LibreSSLVersion\\s+=\\s+\"(?<currentValue>[0-9]*.[0-9]*.[0-9]*)"
      ]
    }
  ],
  "postUpdateOptions": [
    "gomodTidy",
    "gomodUpdateImportPaths"
  ]
}
